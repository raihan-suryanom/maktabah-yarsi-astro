{
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-astro"],
  "importOrder": [
    "^react$",
    "",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "",
    "^[.]/middlewares",
    "^[.]/models",
    "^[.]/controllers",
    "^[.]/services",
    "^[.]/data",
    "^[.]/utils",
    "^[.]",
    "",
    "<TYPES>",
    "<TYPES>^[.]"
  ],
  "overrides": [
    {
      "files": ["*.astro", "*.svg"],
      "options": {
        "parser": "astro"
      }
    }
  ]
}
