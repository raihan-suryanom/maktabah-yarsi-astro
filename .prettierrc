{
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-astro"],
  "importOrder": [
    "^astro$",
    "^react$",
    "",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "",
    "^[~]",
    "",
    "<TYPES>",
    "<TYPES>^[.]"
  ],
  "overrides": [
    {
      "files": ["*.astro", "*.svg"],
      "options": {
        "parser": "astro"
      }
    }
  ]
}
