{
  "name": "maktabah-yarsi",
  "type": "module",
  "author": "Muhammad Raihan Suryanom <muhammad.suryanom@btpn.com> (https://maktabah-yarsi-astro.vercel.app/)",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "disable:toolbar": "astro preferences disable devToolbar",
    "build": "astro check && astro build",
    "fresh:install": "rm -rf node_modules && rm yarn.lock && yarn",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal-2023 public/favicon.svg",
    "preview": "yarn build && vercel deploy --prebuilt",
    "vercel:prod": "vercel --prod",
    "astro": "astro",
    "check": "npx prettier . --check",
    "format": "npx prettier . --write"
  },
  "dependencies": {
    "astro": "4.11.0",
    "astro-icon": "1.1.0",
    "astro-loading-indicator": "0.5.0",
    "class-variance-authority": "0.7.0",
    "react": "18.3.1",
    "react-select": "5.8.0",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@astrojs/check": "0.7.0",
    "@astrojs/react": "3.6.0",
    "@astrojs/tailwind": "5.1.0",
    "@astrojs/vercel": "7.7.1",
    "@ianvs/prettier-plugin-sort-imports": "4.2.1",
    "@iconify-json/lucide": "1.1.196",
    "@tailwindcss/typography": "0.5.13",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "clsx": "2.1.1",
    "prettier": "3.3.2",
    "prettier-plugin-astro": "0.14.0",
    "prettier-plugin-tailwindcss": "0.6.5",
    "react-dom": "18.3.1",
    "sharp-ico": "0.1.5",
    "tailwind-merge": "2.3.0",
    "tailwindcss": "3.4.4",
    "typescript": "5.5.3"
  },
  "resolutions": {
    "sharp": "0.32.6"
  },
  "engines": {
    "node": "20.x"
  },
  "packageManager": "yarn@4.3.0"
}
